local function ENHVH_fake_script() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript', MainFrame)

	local player = game.Players.LocalPlayer -- Get the local player
	local profileFrame = script.Parent:WaitForChild("ProfileFrame") -- Reference the ProfileFrame
	
	-- Locate Name (TextLabel) and ProfileImage (ImageLabel) inside the ProfileFrame
	local nameLabel = profileFrame:FindFirstChild("Name")
	local profileImage = profileFrame:FindFirstChild("ProfileImage")
	
	if nameLabel and profileImage then
		-- Set the player's name
		nameLabel.Text = player.Name
	
		-- Set the player's avatar image
		local avatarUrl = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. player.UserId .. "&width=420&height=420&format=png"
		profileImage.Image = avatarUrl
	else
		warn("Name or ProfileImage is missing in ProfileFrame.")
	end
	
end
coroutine.wrap(ENHVH_fake_script)()
local function HMWN_fake_script() -- ChatBypass.ChatBypassLoader 
	local script = Instance.new('LocalScript', ChatBypass)

	local button = script.Parent
	local function loadInfiniteYield()
	    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end
	button.Activated:Connect(loadInfiniteYield)
end
coroutine.wrap(HMWN_fake_script)()
