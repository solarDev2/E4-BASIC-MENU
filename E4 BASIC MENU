-- Scripts:

local function OOTAVJ_fake_script() -- Home.LocalScript 
	local script = Instance.new('LocalScript', Home)

	-- Reference the main UI elements
	local mainFrame = script.Parent.Parent -- Assuming ScriptHub button is inside MainFrame
	local mainHubFrame = mainFrame.Parent:WaitForChild("MainFrame") -- Assuming ScriptHub frame is at the same level as MainFrame
	local homeHubButton = mainFrame:WaitForChild("Home") -- The ImageButton
	
	-- Toggle functionality
	homeHubButton.MouseButton1Click:Connect(function()
		-- Toggle visibility of the ScriptHub frame
		mainHubFrame.Visible = not mainHubFrame.Visible
	end)
	
end
coroutine.wrap(OOTAVJ_fake_script)()
local function TIGYD_fake_script() -- ProfileImage.LocalScript 
	local script = Instance.new('LocalScript', ProfileImage)

	local Players = game:GetService("Players")
	local GuiService = game:GetService("GuiService")
	
	local userId = Players:GetUserIdFromNameAsync("RobloxUsername")
	
	local userThumbnail, isReady = Players:GetUserThumbnailAsync(userId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
	
	local profileFrame = Instance.new("Frame")
	profileFrame.Name = "ProfileFrame"
	profileFrame.Size = UDim2.new(0, 420, 0, 420)
	profileFrame.Position = UDim2.new(0, 0, 0, 0)
	profileFrame.BackgroundTransparency = 1
	
	profileFrame.BackgroundTransparency = 0
	profileFrame.Image = isReady and userThumbnail or "rbxassetid://0"
	profileFrame.ImageColor3 = Color3.new(1, 1, 1)
	profileFrame.ImageTransparency = 0
	
	gui:WaitForChild("ProfileImage").Parent = profileFrame
	
	local userInfo = Players:GetUserInfosByUserIdsAsync({userId})
	local displayName = userInfo.DisplayName
	local username = userInfo.Username
	
	profileFrame.TextLabel.Text = "Username: " .. username
	profileFrame.TextLabel2.Text = "DisplayName: " .. displayName
end
coroutine.wrap(TIGYD_fake_script)()
local function JITIMPK_fake_script() -- TimeFrame.LocalScript 
	local script = Instance.new('LocalScript', TimeFrame)

	-- Set the duration for 3000 years in seconds
	local totalSeconds = 3000 * 365 * 24 * 60 * 60 -- 3000 years
	
	-- Access the TextLabel named "Time" inside "TimeFrame"
	local timeLabel = script.Parent:WaitForChild("Time")
	
	-- Function to update the countdown
	local function updateCountdown()
		while totalSeconds > 0 do
			-- Calculate years, months, and seconds
			local years = math.floor(totalSeconds / (365 * 24 * 60 * 60))
			local months = math.floor((totalSeconds % (365 * 24 * 60 * 60)) / (30 * 24 * 60 * 60))  -- Using 30 days as a month
			local seconds = totalSeconds % 60  -- Calculate remaining seconds
	
			-- Update the TextLabel with the current countdown
			timeLabel.Text = string.format("%d years %d months %d seconds", years, months, seconds)
	
			-- Wait for 1 second before updating again
			wait(1)
	
			-- Decrease the total time by 1 second
			totalSeconds = totalSeconds - 1
		end
	
		-- When the timer ends, show a message
		timeLabel.Text = "Countdown finished!"
	end
	
	-- Start the countdown
	updateCountdown()
	
end
coroutine.wrap(JITIMPK_fake_script)()
local function VYWQCDL_fake_script() -- ScriptHub.LocalScript 
	local script = Instance.new('LocalScript', ScriptHub)

	-- Reference the main UI elements
	local mainFrame = script.Parent.Parent -- Assuming ScriptHub button is inside MainFrame
	local scriptHubFrame = mainFrame.Parent:WaitForChild("ScriptHub") -- Assuming ScriptHub frame is at the same level as MainFrame
	local scriptHubButton = mainFrame:WaitForChild("ScriptHub") -- The ImageButton
	
	-- Toggle functionality
	scriptHubButton.MouseButton1Click:Connect(function()
		-- Toggle visibility of the ScriptHub frame
		scriptHubFrame.Visible = not scriptHubFrame.Visible
	end)
	
end
coroutine.wrap(VYWQCDL_fake_script)()
local function FKHF_fake_script() -- CloseScriptHub.LocalScript 
	local script = Instance.new('LocalScript', CloseScriptHub)

	-- Reference to the TextButton for closing MainFrame
	local player = game.Players.LocalPlayer
	local screenGui = player.PlayerGui:WaitForChild("ScreenGui")  -- Reference to ScreenGui
	local mainFrame = screenGui:WaitForChild("MainFrame")  -- Reference to MainFrame inside ScreenGui
	
	-- Attempt to get the Close ScriptHub button inside MainFrame
	local closeScriptHubButton = mainFrame:WaitForChild("Close ScriptHub", 5)  -- Wait 5 seconds for the button
	
	-- Debugging to check if the button and frame are found
	if closeScriptHubButton then
		print("Close ScriptHub button found!")
	else
		print("Close ScriptHub button NOT found!")
	end
	
	if mainFrame then
		print("MainFrame found!")
	else
		print("MainFrame NOT found!")
	end
	
	-- Close MainFrame when Close ScriptHub button is clicked
	if closeScriptHubButton and mainFrame then
		closeScriptHubButton.MouseButton1Click:Connect(function()
			print("Closing MainFrame...")
			mainFrame.Visible = false  -- Hide the MainFrame
		end)
	else
		print("Button or Frame is missing!")
	end
	
end
coroutine.wrap(FKHF_fake_script)()
local function SFTSHBR_fake_script() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript', MainFrame)

	-- Create the ScreenGui
	local screenGui = Instance.new("ScreenGui")
	screenGui.Name = "CustomPopup"
	screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui") -- Parent to PlayerGui
	
	-- Create the Frame (popup container) with a rectangle shape
	local frame = Instance.new("Frame")
	frame.Name = "Frame"
	frame.Size = UDim2.new(0, 300, 0, 60)  -- Size of the popup, now more rectangular
	frame.Position = UDim2.new(0.8, 0, 0.9, 0)  -- Position at the bottom-right corner
	frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)  -- Light background color
	frame.BackgroundTransparency = 0.6  -- Slight transparency for a light look
	frame.BorderSizePixel = 2  -- Optional: Add a border
	frame.BorderColor3 = Color3.fromRGB(0, 0, 0)  -- Border color
	frame.Parent = screenGui  -- Parent the frame to the ScreenGui
	
	-- Create the TextLabel to display the message
	local textLabel = Instance.new("TextLabel")
	textLabel.Name = "TextLabel"
	textLabel.Size = UDim2.new(1, -10, 1, -10)  -- Add padding inside the frame
	textLabel.Position = UDim2.new(0, 5, 0, 5)  -- Padding of 5 pixels from the edges
	textLabel.Text = "E4 BASIC MENU ACTIVE"  -- Message to display
	textLabel.TextColor3 = Color3.fromRGB(0, 0, 0)  -- Dark text for contrast
	textLabel.TextSize = 18  -- Text size
	textLabel.TextXAlignment = Enum.TextXAlignment.Center  -- Center the text horizontally
	textLabel.TextYAlignment = Enum.TextYAlignment.Center  -- Center the text vertically
	textLabel.BackgroundTransparency = 1  -- No background for the text itself
	textLabel.TextWrapped = true  -- Wrap text if it doesn't fit in one line
	textLabel.Parent = frame  -- Parent the text label to the frame
	
	-- Function to show the popup and then hide it after 5 seconds
	local function showPopup()
		frame.Visible = true  -- Show the frame
		wait(5)  -- Wait for 5 seconds
		frame:Destroy()  -- Remove the frame after 5 seconds
	end
	
	-- Show the popup
	showPopup()
	
end
coroutine.wrap(SFTSHBR_fake_script)()
